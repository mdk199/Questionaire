<%= form_for(@expertise) do |f| %>
  <%= render "shared/error_messages", :object => @expertise %>

  <div class="field">
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </div>
  <div class="field">
    <%= f.label :expertise %><br />
    <%= f.text_field :expertise %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
